{
	"name": "core/paragraph",
	"category": "common",
	"attributes": {
		"align": {
			"type": "string"
		},
		"content": {
			"type": "string",
			"source": "html",
			"selector": "p",
			"default": ""
		},
		"dropCap": {
			"type": "boolean",
			"default": false
		},
		"placeholder": {
			"type": "string"
		},
		"direction": {
			"type": "string",
			"enum": [
				"ltr",
				"rtl"
			]
		}
	},
	"supports": {
		"className": false,
		"__unstablePasteTextInline": true,
		"lightBlockWrapper": true,
		"__experimentalColor": true,
		"__experimentalLineHeight": true,
		"__experimentalFontSize": true,
		"__experimentalFeatures": {
			"typography": {
				"dropCap": true
			}
		}
	}
}
